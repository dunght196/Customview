<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomClock_XML">
        <attr name="color_strock" format="color"/>
        <attr name="width_strock_circle" format="dimension"/>
        <attr name="x_title" format="dimension"/>
        <attr name="y_title" format="dimension"/>
        <attr name="title" format="string"/>
        <attr name="text_size_title" format="dimension"/>
        <attr name="is_square_width" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="StateView">
        <attr name="sv_loadingView" format="reference" />
        <attr name="sv_emptyView" format="reference" />
        <attr name="sv_errorView" format="reference" />
        <attr name="sv_networkView" format="reference" />
        <attr name="sv_searchView" format="reference" />
        <attr name="sv_viewState" format="enum">
            <enum name="content" value="0" />
            <enum name="error" value="1" />
            <enum name="empty" value="2" />
            <enum name="loading" value="3" />
            <enum name="network" value="4" />
            <enum name="search" value="5" />
        </attr>
    </declare-styleable>
</resources>